<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>


	<class name="com.party.entity.Activities" table="activities">
		<id name="activity_id" type="int">

			 <generator class="native"></generator>
		</id>
		<property name="title" type="java.lang.String"></property>
		<property name="contents" type="java.lang.String"></property>
		<property name="time_begin" type="date"></property>
		<property name="time_over" type="date"></property>
		<property name="statu" type="java.lang.String"></property>
		<property name="num" type="int"></property>
		<property name="teacher" type="java.lang.String"></property>


	</class>
</hibernate-mapping>